@model IEnumerable<ReportMapping>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script src="~/vendor/jquery/jquery.js"></script>

<h3 style="margin-left:10px;">Dashboard</h3>
<script type="text/javascript">
        //$(document).ready(function () {
        //    alert("ready!");
        //});
</script>

<div class="row">
    @foreach (var p in Model)
    {
        <div class="col-xl-3 col-md-6 mb-4">
            <div class="card border-left-primary shadow h-100 py-2">
                <div class="card-body">
                    <div class="row no-gutters align-items-center">
                        <div class="col mr-2">
                            <div class="text-xs font-weight-bold text-primary text-uppercase mb-1">@p.ReportTitle</div>
                            @*<div class="h5 mb-0 font-weight-bold text-gray-800">@p.ReportUrl</div>*@
                            <div class="h5 mb-0 font-weight-bold text-gray-800"><a href="@p.ReportUrl" target="_blank">Link</a></div>

                        </div>
                    </div>
                </div>
            </div>
        </div>
    }
</div>




